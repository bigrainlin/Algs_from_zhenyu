    "class Solution(object):\n",
    "    def reverse(self, x):\n",
    "        if x>=0:\n",
    "            s=1\n",
    "        else:\n",
    "            s=-1\n",
    "        r = int(str(s*x)[::-1])\n",
    "        return s*r * (r < 2**31)"
   ]
  },
